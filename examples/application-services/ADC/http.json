{
    "class": "AS3",
    "action": "deploy",
    "persist": true,
    "declaration": {
        "class": "ADC",
        "schemaVersion": "3.9.0",
        "id": "MyTenant_HTTP",
        "label": "MyTenant_HTTP",
        "remark": "MyTenant_HTTP HTTP Service",
        "MyTenant_HTTP": {
            "class": "Tenant",
            "MyApp": {
                "class": "Application",
                "template": "generic",
                "MyService": {
                    "class": "Service_HTTP",
                    "virtualAddresses": [
                        "10.1.10.100"
                    ],
                    "virtualPort": 80,
                    "pool": "web_pool"
                },
                "web_pool": {
                    "class": "Pool",
                    "monitors": [
                        "http"
                    ],
                    "members": [
                        {
                            "servicePort": 80,
                            "serverAddresses": [
                                "10.1.20.11"
                            ]
                        }
                    ]
                }
            }
        }
    }
}
{
    "$schema": "https://raw.githubusercontent.com/F5Networks/f5-appsvcs-extension/master/schema/latest/as3-schema-3.9.0-3.json",
    "class": "AS3",
    "action": "deploy",
    "persist": true,
    "declaration": {
        "class": "ADC",
        "schemaVersion": "3.9.0",
        "id": "{{tenant_name}}",
        "label": "{{tenant_name}}",
        "remark": "{{tenant_name}} TCP Fast L4 APP",
        "{{tenant_name}}": {
            "class": "Tenant",
            "{{service_name}}_app": {
                "class": "Application",
                "template": "generic",
                "{{service_name}}": {
                    "class": "Service_L4",
                    "persistenceMethods": [
                        {
                            "use": "{{tenant_name}}_persistence"
                        }
                    ],
                    "virtualPort": 22,
                    "virtualAddresses": [
                        "10.1.1.100"
                    ],
                    "layer4": "tcp",
                    "profileL4": {
                        "use": "{{tenant_name}}_fastL4_tcp"
                    },
                    "pool": "{{tenant_name}}_pool",
                    "snat": {
                        "bigip": "/Common/snat-pool"
                    }
                },
                "{{tenant_name}}_persistence": {
                    "class": "Persist",
                    "persistenceMethod": "source-address",
                    "duration": 1800,
                    "mirror": true
                },
                "{{tenant_name}}_fastL4_tcp": {
                    "class": "L4_Profile",
                    "idleTimeout": 300,
                    "looseClose": true,
                    "looseInitialization": true,
                    "resetOnTimeout": false,
                    "tcpCloseTimeout": 30,
                    "tcpHandshakeTimeout": 30
                },
                "{{tenant_name}}_pool": {
                    "class": "Pool",
                    "loadBalancingMode": "least-connections-member",
                    "slowRampTime": 10,
                    "monitors": [
                        {
                            "use": "{{tenant_name}}_mon"
                        }
                    ],
                    "members": [
                        {
                            "servicePort": 22,
                            "serverAddresses": [
                                "10.2.2.201",
                                "10.2.2.202"
                            ]
                        }
                    ]
                },
                "{{tenant_name}}_mon": {
                    "class": "Monitor",
                    "monitorType": "tcp",
                    "targetPort": 22,
                    "send": "",
                    "receive": ""
                }
            }
        }
    }
}
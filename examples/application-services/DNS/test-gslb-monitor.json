{
    "$schema": "https://raw.githubusercontent.com/F5Networks/f5-appsvcs-extension/master/schema/latest/as3-schema-3.9.0-3.json",
    "class": "ADC",
    "schemaVersion": "3.9.0",
    "id": "gslbmonitorudp",
    "remark": "GSLB monitor test",
    "controls": {
        "class": "Controls",
        "trace": true,
        "logLevel": "debug"
    },
    "Gslb_UDP_Monitor": {
        "class": "Tenant",
        "Application": {
            "class": "Application",
            "template": "generic",
            "gslbUdpMonitor": {
                "class": "GSLB_Monitor",
                "remark": "A GLSB UDP Monitor",
                "monitorType": "udp",
                "target": "1.1.1.1:80",
                "interval": 100,
                "timeout": 1000,
                "probeTimeout": 110,
                "ignoreDownResponseEnabled": true,
                "transparent": true,
                "probeInterval": 33,
                "probeAttempts": 25,
                "debugEnabled": true,
                "reverseEnabled": true,
                "send": "HEAD / HTTP/1.0\\r\\n",
                "receive": "HTTP"
            }
        }
    }
}
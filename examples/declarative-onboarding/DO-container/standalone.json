{
    "class": "DO",
    "targetHost": "192.168.1.147",
    "targetPort": 8443,
    "targetUsername": "admin",
    "targetPassphrase": "admin",
    "declaration": {
        "class": "Device",
        "schemaVersion": "1.0.0",
        "async": "true",
        "label": "BIG-IP declarative onboarding declaration",
        "Common": {
            "class": "Tenant",
            "hostname": "bigip1.lab.local",
            "myDns": {
                "class": "DNS",
                "nameServers": [
                    "8.8.8.8"
                ],
                "search": [
                    "lab.local"
                ]
            },
            "myNtp": {
                "class": "NTP",
                "servers": [
                    "0.pool.ntp.org",
                    "1.pool.ntp.org"
                ],
                "timezone": "EST"
            },
            "guestUser": {
                "class": "User",
                "userType": "regular",
                "password": "guestNewPass1",
                "partitionAccess": {
                    "Common": {
                        "role": "guest"
                    }
                }
            },
            "anotherUser": {
                "class": "User",
                "userType": "regular",
                "password": "myPass1word",
                "shell": "none",
                "partitionAccess": {
                    "all-partitions": {
                        "role": "guest"
                    }
                }
            },
            "myProvisioning": {
                "class": "Provision",
                "ltm": "nominal",
                "afm": "nominal"
            },
            "dbvars": {
                "class": "DbVariables",
                "ui.advisory.enabled": true,
                "ui.advisory.color": "green",
                "ui.advisory.text": "/Common/hostname"
            }
        }
    }
}
{
    "class": "AS3",
    "action": "deploy",
    "declaration": {
        "class": "ADC",
        "schemaVersion": "3.0.0",
        "id": "container",
        "label": "Generic TCP Service",
        "remark": "Simple TCP based service",
        "TCP_App": {
            "class": "Tenant",
            "A1": {
                "class": "Application",
                "template": "http",
                "serviceMain": {
                    "class": "Service_HTTP",
                    "virtualAddresses": [
                        "10.0.1.10"
                    ],
                    "pool": "tcp_pool"
                },
                "tcp_pool": {
                    "class": "Pool",
                    "monitors": [
                        "tcp"
                    ],
                    "members": [
                        {
                            "servicePort": 22,
                            "serverAddresses": [
                                "192.0.1.10"
                            ]
                        }
                    ]
                }
            }
        }
    }
}